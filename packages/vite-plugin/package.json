{
  "name": "vite-plugin-imgx",
  "type": "module",
  "version": "0.1.6",
  "description": "Vite plugin for imgx",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/imgx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/imgx.git"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/imgx/issues"
  },
  "keywords": [
    "image",
    "vite",
    "plugin",
    "manipulation",
    "compression",
    "web optimization",
    "jpg",
    "png",
    "avif",
    "webp",
    "sharp",
    "bun",
    "stacks",
    "typescript",
    "javascript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["README.md", "dist"],
  "scripts": {
    "build": "bun build.ts",
    "lint": "bunx --bun eslint .",
    "lint:fix": "bunx --bun eslint . --fix",
    "fresh": "bunx rimraf node_modules/ bun.lock && bun i",
    "prepublishOnly": "bun --bun run build",
    "release": "bun run changelog && bunx bumpp package.json --all",
    "test": "bun test",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@stacksjs/imgx": "workspace:*"
  },
  "devDependencies": {
    "bun-plugin-dtsx": "^0.21.13"
  },
  "simple-git-hooks": {
    "pre-commit": "bunx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": "bunx eslint . --fix"
  }
}
